    rel="preload"
    as="font"
    href="{{ settings.type_accent_font | font_url }}"
    type="font/woff2"
    crossorigin
    fetchpriority="low"
  >
{%- endunless -%}
{% # theme-check-enable %}

<style>
  @font-face {
  font-family: "Matter Trial";
  src: url({{ "MatterTRIAL-Regular_(1)" |  asset_url }}) format("woff2");
  font-weight: 400;
  font-style: normal;
}

* {
    font-family: "Matter Trial";
}
</style>